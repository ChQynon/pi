{
  "version": 2,
  "builds": [
    {
      "src": "webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook/(.*)",
      "dest": "webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "webhook.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
} 